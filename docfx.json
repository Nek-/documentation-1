{
  "metadata": [
    {
      "src": [
        {
          "src": "../EventStore",
          "files": [
            "src/**.csproj"
          ]
        }
      ],
      "dest": "code",
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": "http-api/swagger.json"
      },
      {
        "files": [
          "code/**.yml",
          "dotnet-api/**.md",
          "event-sourcing-basics/**.md",
          "http-api/**.md",
          "introduction/**.md",
          "projections/**.md",
          "server/**.md",
          "*.md",
          "toc.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "assets/**"
        ]
      }
    ],
    "dest": "_site",
    "exclude": [
      ".git/**"
    ],
      "template": [
        "../es-docs-template",
        "../dfx-plugins/rest.tagpage.2.31.0/content",
        "../dfx-plugins/rest.operationpage.2.31.0/content"
      ]
  }
}

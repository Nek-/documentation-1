{
  "metadata": [
    {
      "src": [
        {
          "src": "../EventStore",
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "docs/**"
          ]
        }
      ],
      "dest": "docs/code",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": "http-api/swagger.json",
        "dest":"docs/http-api"
      },
      {
        "files": [
          "dotnet-api/**.md",
          "event-sourcing-basics/**.md",
          "http-api/**.md",
          "introduction/**.md",
          "projections/**.md",
          "server/**.md",
          "*.md"
        ],
        "exclude": [],
        "dest":"docs"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "docs/**",
          ".git/**"
        ]
      }
    ],
    "overwrite": [],
    "dest": "docs",
    "exclude": [
      ".git/**"
    ],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],

      "template": [
          "../es-docs-template",
          "../dfx-plugins/rest.tagpage.2.31.0/content",
          "../dfx-plugins/rest.operationpage.2.31.0/content"
      ],

    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "disableBreadcrumbs": false
  }
}
